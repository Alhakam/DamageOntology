###  https://w3id.org/dco#description_content
:description_content rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Description ;
                     rdfs:comment "Textual content used for the Description of a Structure Element or Damage."@en ;
                     rdfs:label "description content"@en .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/dco#Component
:Component rdf:type owl:Class ;
           rdfs:subClassOf :Structure_Element ;
           owl:disjointWith :Structure ;
           rdfs:comment "Object that is part of a structure."@en ;
           rdfs:label "Component"@en .


###  https://w3id.org/dco#Damage
:Damage rdf:type owl:Class ;
        owl:disjointWith :Documentation ,
                         :Structure_Element ;
        rdfs:comment "Damage is the harm to a structure resulting in impairment or loss of function, usefulness or value."@en ;
        rdfs:label "Damage"@en .


###  https://w3id.org/dco#Damage_Area
:Damage_Area rdf:type owl:Class ;
             rdfs:subClassOf :Damage ;
             owl:disjointWith :Damage_Pattern ,
                              :Individual_Damage ;
             rdfs:comment "Area in a component where damage is located."@en ;
             rdfs:label "Damage Area"@en .


###  https://w3id.org/dco#Damage_Pattern
:Damage_Pattern rdf:type owl:Class ;
                rdfs:subClassOf :Damage ;
                owl:disjointWith :Individual_Damage ;
                rdfs:comment "Defines a group of physically connected individual damage elements."@en ;
                rdfs:label "Damage Pattern"@en .


###  https://w3id.org/dco#Defect
:Defect rdf:type owl:Class ;
        rdfs:subClassOf :Damage ;
        owl:disjointWith :Structural_Damage ;
        rdfs:comment "Damage that does not impair the structural integrity of the structure, but can still have influence on the durability, usability and / or value."@en ;
        rdfs:label "Defect"@en .


###  https://w3id.org/dco#Description
:Description rdf:type owl:Class ;
             rdfs:subClassOf :Documentation ;
             rdfs:comment "Short textual documentation about a Damage or Structure Element. Generally created by the inspecting person."@en ;
             rdfs:label "Description"@en .


###  https://w3id.org/dco#Documentation
:Documentation rdf:type owl:Class ;
               owl:disjointWith :Structure_Element ;
               rdfs:comment "Material or data that provides information or evidence about the structure and its components or that serves as a record."@en ;
               rdfs:label "Documentation"@en .


###  https://w3id.org/dco#External_Ressource
:External_Ressource rdf:type owl:Class ;
                    rdfs:subClassOf :Documentation ;
                    rdfs:comment "Document that is stored in a separate file, referenced by an external path."@en ;
                    rdfs:label "External Ressource"@en .


###  https://w3id.org/dco#Individual_Damage
:Individual_Damage rdf:type owl:Class ;
                   rdfs:subClassOf :Damage ;
                   rdfs:comment "Damage that is modelled on a very detailled level and belongs with other adjacent individual damage elements to a broader damage area."@en ;
                   rdfs:label "Individual Damage"@en .


###  https://w3id.org/dco#Structural_Damage
:Structural_Damage rdf:type owl:Class ;
                   rdfs:subClassOf :Damage ;
                   rdfs:comment "Damage that impairs the structural integrity of the structure."@en ;
                   rdfs:label "Structural Damage"@en .


###  https://w3id.org/dco#Structure
:Structure rdf:type owl:Class ;
           rdfs:subClassOf :Structure_Element ;
           rdfs:comment "Something (such as a building or a bridge) that is constructed."@en ;
           rdfs:label "Structure"@en .


###  https://w3id.org/dco#Structure_Element
:Structure_Element rdf:type owl:Class ;
                   rdfs:comment "Objects that are damaged."@en ;
                   rdfs:label "Structure Element"@en ;
                   owl:deprecated "Perhaps replace with bot:Element and bot:Zone ? (discussion)"@en .


