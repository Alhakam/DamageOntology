#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/dco#combinesIndividualDamage
:combinesIndividualDamage rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf owl:topObjectProperty ;
                          rdfs:domain :Damage_Pattern ;
                          rdfs:range :Individual_Damage ;
                          owl:propertyChainAxiom ( :combinesIndividualDamage
                                                   :connectsDamage
                                                 ) ;
                          rdfs:comment "Combines Individual Damage parts that are physically connected in a Damage Pattern."@en ,
                                       "Recommended Cardinality: min 2"@en ;
                          rdfs:label "combines Individual Damages"@en .


###  https://w3id.org/dco#connectsDamage
:connectsDamage rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ;
                rdfs:domain :Damage ;
                rdfs:range :Damage ;
                rdfs:comment "Defines the relationship between two damage individuals that are physically connected with each other."@en ;
                rdfs:label "connects damage"@en .


###  https://w3id.org/dco#contains
:contains rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty ;
          rdfs:comment "A relationship between an object and its constituent part(s)."@en ;
          rdfs:label "contains"@en .


###  https://w3id.org/dco#hasComponent
:hasComponent rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :contains ;
              rdfs:domain :Structure ;
              rdfs:range :Component ;
              rdfs:comment "The relationship between a structure and its component."@en ;
              rdfs:label "has component"@en .


###  https://w3id.org/dco#hasDamage
:hasDamage rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :contains ;
           rdf:type owl:TransitiveProperty ;
           rdfs:range :Damage ;
           owl:propertyChainAxiom ( :hasComponent
                                    :hasDamage
                                  ) ,
                                  ( :hasDamage
                                    :connectsDamage
                                  ) ,
                                  ( :hasPart
                                    :hasDamage
                                  ) ;
           rdfs:comment "The relationship between an individual and its contained damage individuals"@en ;
           rdfs:label "has damage"@en .


###  https://w3id.org/dco#hasDamageArea
:hasDamageArea rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :hasDamage ;
               rdfs:range :Damage_Area ;
               rdfs:comment "A relationship between an individual (Component recommended) and its consisting Damage Area individuals."@en ;
               rdfs:label "has Damage Area"@en .


###  https://w3id.org/dco#hasDamagePattern
:hasDamagePattern rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasDamage ;
                  rdfs:range :Damage_Pattern ;
                  rdfs:comment "A relationship between an individual (Damage Area recommended) and its consisting Damage Pattern."@en ;
                  rdfs:label "has Damage Pattern"@en .


###  https://w3id.org/dco#hasDocumentation
:hasDocumentation rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:range :Documentation ;
                  rdfs:comment "Relationship between a Documenation and the relevant documented or inspected object which could be either a Damage or a Structure Element."@en ;
                  rdfs:label "has Documentation"@en .


###  https://w3id.org/dco#hasIndividualDamage
:hasIndividualDamage rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasDamage ;
                     rdfs:range :Individual_Damage ;
                     owl:propertyChainAxiom ( :hasDamageArea
                                              :combinesIndividualDamage
                                            ) ;
                     rdfs:comment "A relationship between an individual (Damage Area or Damage Pattern recommended) and its consisting Individual Damage elements"@en ;
                     rdfs:label "has Individual Damage"@en .


###  https://w3id.org/dco#hasPart
:hasPart rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :contains ;
         rdf:type owl:TransitiveProperty ;
         rdfs:domain :Component ;
         rdfs:range :Component ;
         rdfs:comment "A relationship between a component and its sub-parts (which are also components)."@en ;
         rdfs:label "has part"@en ;
         owl:deprecated "should be handled in other ontologies for component / building element topology"@en .


