###  https://w3id.org/damagemodels/cdo#area
:area rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf :measurements ;
      rdfs:domain :Chemical_Damage ,
                  :Spalling ;
      rdfs:comment "Defines the area of the Damage Area (used for Chemical Damage or Spalling)"@en ;
      rdfs:label "area" .


###  https://w3id.org/damagemodels/cdo#depth
:depth rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf :measurements ;
       rdfs:domain :Crack ;
       rdfs:comment "Defines the crack depth"@en ;
       rdfs:label "depth"@en .


###  https://w3id.org/damagemodels/cdo#measurements
:measurements rdf:type owl:DatatypeProperty ;
              rdfs:comment "Simple measurement values that could be attached to the Damage Elements for queries."@en ;
              rdfs:label "measurements"@en .


###  https://w3id.org/damagemodels/cdo#spacing
:spacing rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :measurements ;
         rdfs:domain :Crack ;
         rdfs:comment "Defines the crack spacing."@en ;
         rdfs:label "spacing" .


